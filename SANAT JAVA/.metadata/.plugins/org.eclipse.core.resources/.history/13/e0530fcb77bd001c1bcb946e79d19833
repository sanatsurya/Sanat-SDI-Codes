
public class Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a =4;
		switch(a) {
		case 1:
			System.out.println("This is one");
		case 2:
			System.out.println("This is two");
		case 3:
			System.out.println("This is three");
		case 4:
			System.out.println("This is four");
		}

	}

}
